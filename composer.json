{
  "name": "lyenrowe/omnipay-alipay",
  "type": "library",
  "description": "Alipay gateway for Omnipay payment processing library",
  "keywords": [
    "alipay",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "purchase"
  ],
  "homepage": "https://github.com/lyenrowe/omnipay-alipay",
  "license": "MIT",
  "authors": [
    {
      "name": "lyenrowe",
      "email": "luolaiqing@leying365.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Omnipay\\Alipay\\": "src/",
      "Omnipay\\Alipay\\Tests\\": "tests/"
    },
    "files": [
      "src/Common/helpers.php"
    ]
  },
  "require": {
    "omnipay/common": "v3.0-alpha.1"
  },
  "require-dev": {
    "omnipay/tests": "v3.0-alpha.1",
    "squizlabs/php_codesniffer": "3.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
